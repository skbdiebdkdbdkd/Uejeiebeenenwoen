    workspace.Gravity = 196.2

    hm.PlatformStand = false

    

    

    for _, animTrack in ipairs(hm:GetPlayingAnimationTracks()) do

    if animTrack.Animation.AnimationId == "rbxassetid://15392759696" then

      animTrack:Stop()

    end

   end

  end

end







