local function ChangeAnims(idle,walk,run,jump,fall,swim)

Animate.idle:ClearAllChildren()
 Animate.walk:ClearAllChildren()
 Animate.run:ClearAllChildren()
 Animate.jump:ClearAllChildren()
 Animate.fall:ClearAllChildren()
 Animate.swim:ClearAllChildren()





local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. idle
animation.Parent = Animate.idle

local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. walk
animation.Parent = Animate.walk

local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. run
animation.Parent = Animate.run

local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. jump
animation.Parent = Animate.jump

local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. fall
animation.Parent = Animate.fall

local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://".. swim
animation.Parent = Animate.swim

end





